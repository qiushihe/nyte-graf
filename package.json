{
  "name": "test",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm-run-all build:clean build:prep build:tsc build:alias",
    "build:clean": "shx rm -rf ./build",
    "build:prep": "shx mkdir ./build",
    "build:tsc": "tsc --project ./tsconfig.json",
    "build:alias": "tsc-alias --project tsconfig.json",
    "dev": "npm-run-all build dev:run",
    "dev:run": "node ./build/main.js",
    "test": "npm-run-all test:lint:fix test:tsc test:unit",
    "test:tsc": "tsc --noemit --skipLibCheck",
    "test:lint": "eslint --ext .ts ./src",
    "test:lint:fix": "eslint --ext .ts --fix ./src",
    "test:unit": "echo LOL"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  }
}
